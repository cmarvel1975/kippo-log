    var currentUserId;
    function checkUser(){
        var newValueOfUserId = $.cookie("Login");
        if (newValueOfUserId != currentUserId){
            if (currentUserId != "\"\""){
                alert('\u30e6\u30fc\u30b6\u30fc\u304c\u5909\u66f4\u3055\u308c\u305f\u305f\u3081\u3001\u30e1\u30a4\u30f3\u30da\u30fc\u30b8\u3078\u79fb\u52d5\u3057\u307e\u3059\u3002  ');
                document.location.href = "http://www.4shared.com/";
            }
        }
    }

    function setIntervalForUserChecking(){
        currentUserId = $.cookie("Login");
        //setInterval('checkUser()', 10000);
    }
</script>






<script type="text/javascript">
    function sendVerificationLetter(){
        $.get('http://www.4shared.com/account/sendEmailVerification.jsp?callback=?&globSysLang=ja', {ajax: 'true'} ,function(data){
            var msg;
            if (data.ok) {
                msg = data.verificationMailSentMsg;
            }else{
                msg = data.errorMessage;
            }
            PopupModule.popupWithButtons({title:"Check your e-mail", content:msg});
        }, "jsonp");
    }


    function sendVerificationLetterAndGoToVerificationPage(){
        $.getJSON('/account/sendEmailVerification.jsp?globSysLang=ja', {ajax: 'true'} ,function(data){
            var msg;
            if(data.ok && data.ok === true){
                msg = data.verificationMailSentMsg;
            }else{
                msg = data.errorMessage;
            }
            alert(msg);
            window.location.href = 'http://www.4shared.com/web/acc/verifyemail' ;
        });
    }

</script>



  


































  


































  






