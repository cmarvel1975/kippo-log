  var subdomainInput = '<div class="input-light-small"><input type="text" value="" readonly="readonly" onclick="this.focus();this.select()"'+
      ' class="field1" style="width:390px;" dir="ltr" id="subdomainNameText"/></div>';
  var subDomainControls = '<a href="javascript:void(0)" onclick="quickEditEdit(event,6)" class="d1subdomainControls lucida f13 no-line">編集</a>&nbsp;&nbsp;&nbsp;'+
      '<a href="javascript:void(0)" onclick="removeSubdomain()" class="d1subdomainControls lucida f13 no-line">削除</a>';

  var subDomainAdd = '<a href="javascript:void(0)" onclick="addSubdomain(event)" class="lucida">追加</a>';

  function removeSubdomain() {
    if (confirm('Remove subdomain from current file?')) {
      $.post("/servlet/uifd", {mode:6,fid:3863774691,newValue:''}, function(data) {
        if (data.error) {
          alert(data.error);
          return;
        }
        if (data.valueFull != 'undefined') {
          values[6] = data.valueFull;
        } else {
          values[6] = data.valuePage;
        }
        alert('Subdomain removed');
        document.getElementById("subdomainCell").innerHTML = subDomainAdd;
      }, "json");
    }
  }

  function addSubdomain(event) {
    quickEditEdit(event, 6);
  }


