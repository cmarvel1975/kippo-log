<script type="text/javascript">
    try {
      _gaq.push(['_trackEvent', 'd1-guest', 'PageView' , 'general']);
    } catch (err) { }

    function clickFunction() {
      var element = $(this).attr('data-element')
      if (element)
        try {
        
          _gaq.push(['_trackEvent', 'd1-guest', 'general', element])
        } catch (err) {
        }
    }

    function handlerClick() {
      $('.gaClick, .tab').unbind('click', clickFunction).click(clickFunction)
    }

    $(function() {
      handlerClick()

      if(typeof(Events)!='undefined') Events.addListener('folder.reload', function() {handlerClick()})
    })

    function showGANumbers() {
      $('.gaClick, .tab').each(function () {
        var element = $(this).attr('data-element')
        if ($(this).css('position') != 'absolute') {
          $(this).css('position', 'relative')
        }
        if (element) $(this).append($('<div/>').html(element).addClass('gaHelperNumbers'))
      });
    }
</script>

