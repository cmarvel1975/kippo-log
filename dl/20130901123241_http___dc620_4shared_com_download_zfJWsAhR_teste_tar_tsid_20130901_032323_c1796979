<script type="text/javascript">
  $("#btnLink").click(checkTermsOfUseAndStartDownload)

  function confirmDownloadVirusFile() {
    var infected = false
    return infected ? confirm("\u3053\u306e\u30d5\u30a1\u30a4\u30eb\u306f\u611f\u67d3\u3057\u3066\u3044\u307e\u3059\u3002\u30c0\u30a6\u30f3\u30ed\u30fc\u30c9\u3092\u7d9a\u884c\u3057\u307e\u3059\u304b\uff1f  ") : true
  }

  function startDownloadVirusFile() {
    if (confirmDownloadVirusFile())
      document.location.href = "http:\/\/www.4shared.com\/get\/zfJWsAhR\/teste.html"
  }

  /* terms of use and download */
  function checkTermsOfUseAndStartDownload(event) {
    false ? showTermsOfUse() : checkAndStartDownload(event)

    function showTermsOfUse() {
      if (confirmDownloadVirusFile()) {
        var termPopup = PopupModule.popup({ title:'\u8b66\u544a', element:$("#popupBox") });
        Cookies.create('termsOfUse', '1', 365, '/', '4shared.com');
        handleTermsOfUsePopup(termPopup)
      }
    }

    function handleTermsOfUsePopup(popup) {
      $('.jsPopupBoxOk', popup).click(function(event) {
        hideTermsOfUse(popup)
        checkAndStartDownload(event)
      })

      $('.jsPopupBoxCancel', popup).click(function(event) {
        hideTermsOfUse(popup)
      })
    }

    function checkAndStartDownload(event) {
      if (false)
        return confirmDownloadVirusFile() && promptReactivateAccount(event)
      if (false)
        return confirmDownloadVirusFile() && Authenticate.showPopup({returnTo:'http://dc620.4shared.com/download/zfJWsAhR/teste.tar?tsid=20130901-032323-c1796979', title:"<div>You should log in to download this file.<br /> Sign up for free if you don't have an account yet.</div>"})
      startDownloadVirusFile()
    }

    function hideTermsOfUse(popup) {
      popup.closePopup();
    }

    return false
  }
</script>

  






  





















