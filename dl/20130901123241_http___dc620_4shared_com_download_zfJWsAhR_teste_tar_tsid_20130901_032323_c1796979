<script type="text/javascript">
  var qrCodeShown = false;

  function showQrCode() {
    var codeBody = $("#qrCodeBody");
    var codeTitle = $("#qrCodeTitle");
    if (qrCodeShown) {
      codeBody.slideUp('fast');
      qrCodeShown = false;
      codeTitle[0].innerHTML = "QR\u30b3\u30fc\u30c9\u3092\u8868\u793a";
    } else {
      if (codeBody.size() == 0) {
        createQrCodeTd();
      }
      codeBody = $("#qrCodeBody");
      codeBody.slideDown('fast');
      codeTitle[0].innerHTML = "QR\u30b3\u30fc\u30c9\u3092\u975e\u8868\u793a";
      qrCodeShown = true;
    }
  }

  function createQrCodeTd() {
    var qrCodeBody = $('<div>', {
      id: 'qrCodeBody',
      'class': 'qrCodeBody clearFix paddingR20 f11',
      html: '<b>QRコードとは？</b>このQRコードを使えば、アドレスを入力しなくてもこのページを携帯電話で簡単に閲覧できます。携帯電話でスキャンするだけでOKです！',
      style: 'display: none'
    });

    var qrCodeImg = $('<img>', {
      id: 'qrCodeImg',
      'class': 'floatLeft',
      src: '/main/qrcode?file=zfJWsAhR',
      width: '150',
      height: '150'
    });

    qrCodeBody.prepend(qrCodeImg);
    $('#qrCodeWrapper').append(qrCodeBody);
  }
</script>
